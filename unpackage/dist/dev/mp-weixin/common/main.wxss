/*带输入框弹窗*/
.modal-mask {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: #000;
	opacity: 0.5;
	overflow: hidden;
	z-index: 11;
	color: #fff;
}
.modal-dialog {
	width: 600rpx;
	height: 280rpx;
	overflow: hidden;
	position: fixed;
	top: calc(50% - 140rpx);
	left: calc(50% - 300rpx);
	z-index: 12;
	background: #fff;
	border-radius: 5rpx;
}
.modal-dialog .modal-title {
	padding: 30rpx 0 20rpx 0;
	color: #030303;
	text-align: center;
}
.modal-dialog .modal-content {
	padding: 10rpx;
}
.modal-dialog .input {
	width: 100%;
	height: 60rpx;
	line-height: 28rpx;
	padding: 0 70rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	color: #333;
}
.modal-dialog input-holder {
	color: #666;
}
.modal-dialog .modal-footer {
	width: 100%;
	bottom: 0;
	height: 86rpx;
	border-top: 1px solid #dedede;
	font-size: 36rpx;
	line-height: 86rpx;
}
.modal-dialog .btn-cancel {
	width: 50%;
	color: #000;
	text-align: center;
	border-right: 1px solid #dedede;
}
.modal-dialog .btn-confirm {
	width: calc(50% - 1px);
	color: #5fcf48;
	text-align: center;
}
